/**
 * UI router etc
 */

const app = angular.module('app', ['ui.router']);

app.config(function($stateProvider, $urlRouterProvider){
	
	$urlRouterProvider.otherwise('/login');
	
	$stateProvider
		//home states and nested views
		.state('login',{
			url: '/login',
			templateUrl:'partials/login.html'
		});
	
	
	
});